[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.4
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://addons/godot_rust/target/debug/libgodot_rust.so"
linux.release.x86_64 =   "res://addons/godot_rust/target/release/libgodot_rust.so"
windows.debug.x86_64 =   "res://addons/godot_rust/target/debug/godot_rust.dll"
windows.release.x86_64 = "res://addons/godot_rust/target/release/godot_rust.dll"
macos.debug =            "res://addons/godot_rust/target/debug/libgodot_rust.dylib"
macos.release =          "res://addons/godot_rust/target/release/libgodot_rust.dylib"
macos.debug.arm64 =      "res://addons/godot_rust/target/debug/libgodot_rust.dylib"
macos.release.arm64 =    "res://addons/godot_rust/target/release/libgodot_rust.dylib"
